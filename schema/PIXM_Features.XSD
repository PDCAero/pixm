<?xml  version='1.0' encoding='utf-8' ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	xmlns:aixm="http://www.aixm.aero/schema/5.1.1"
	targetNamespace="http://www.pixm.aero/schema/0.1"
	xmlns="http://www.pixm.aero/schema/0.1"
	xmlns:pixm="http://www.pixm.aero/schema/0.1"
	xmlns:gml="http://www.opengis.net/gml/3.2"
	elementFormDefault="qualified">
	<xs:import namespace="http://www.opengis.net/gml/3.2" schemaLocation="http://schemas.opengis.net/gml/3.2.1/gml.xsd"/>
	<xs:import namespace="http://www.aixm.aero/schema/5.1.1" schemaLocation="http://www.aixm.aero/schema/5.1.1/aixm511.xsd"/>
	<xs:include schemaLocation="PIXM_Codelists.xsd"/>
	<xs:include schemaLocation="PIXM_DataTypes.xsd"/>
	<xs:include schemaLocation="PIXM_Measures.xsd"/>
	<xs:include schemaLocation="PIXM_AbstractGML_ObjectTypes.xsd"/>
	
	<xs:element name="ContaminationTimeSlice" type="pixm:ContaminationTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/> 
	<xs:complexType name="ContaminationTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="contamDepth" type="pixm:ValDepthType" minOccurs="0"/>
					<xs:element name="contamSpecificGravity" type="xs:decimal" minOccurs="0"/>
					<xs:element name="contamType" type="pixm:CodeContaminationType" minOccurs="0"/>
					<xs:element name="muValue" type="pixm:ValMuType" minOccurs="0"/>
					<xs:element name="rcrValue" type="xs:positiveInteger" minOccurs="0"/>
					<xs:element name="location" type="xs:string" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="ContaminationDefinition" type="pixm:CodeContaminationType" substitutionGroup="pixm:AbstractPIXMFeatureType"/> 	
	<xs:complexType name="ContaminationDefinitionType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:ContaminationTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="ObstacleTimeSlice" type="pixm:ObstacleTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/> 
	<xs:complexType name="ObstacleTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="aglHeight" type="pixm:ValDistanceVerticalType" minOccurs="1"/>
					<xs:element name="amslHeight" type="ValDistanceVerticalType" minOccurs="1"/>
					<xs:element name="horizontalAccuracy" type="ValDistanceType" minOccurs="0"/>
					<xs:element name="name" type="xs:string" minOccurs="0"/>
					<xs:element name="studyNumber" type="xs:string" minOccurs="1"/>
					<xs:element name="type" type="xs:string" minOccurs="0"/>
					<xs:element name="verticalAccuracy" type="ValDistanceVerticalType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:element name="Obstacle" type="pixm:ObstacleType" substitutionGroup="pixm:AbstractPIXMFeatureType"/> 	
	<xs:complexType name="ObstacleType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:ObstacleTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:element name="PerformanceObstacleTimeSlice" type="pixm:PerformanceObstacleTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/> 
	<xs:complexType name="PerformanceObstacleTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="heightAboveReference" type="pixm:ValDistanceVerticalType" minOccurs="1"/>
					<xs:element name="longitudinalDistance" type="pixm:ValDistanceType" minOccurs="1"/>
					<xs:element name="lateralDistance" type="pixm:ValDistanceType" minOccurs="0"/>
					<xs:element name="obstacleDatum" type="pixm:CodeObstacleDatumType" minOccurs="0"/>
					<xs:choice minOccurs="0">
						<xs:element name="sourceObstacle" type="ObstacleType"/>
						<xs:element name="sourceAIXMObstacle" type="VerticalStructure"/>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>	
	</xs:complexType>
	
	<xs:element name="PerformanceObstacle" type="pixm:PerformanceObstacleType" substitutionGroup="pixm:AbstractPIXMFeatureType"/> 	
	<xs:complexType name="PerformanceObstacleType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:PerformanceObstacleTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:element name="PerformanceObstacleSetTimeSlice" type="pixm:PerformanceObstacleSetTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/> 
	<xs:complexType name="PerformanceObstacleSetTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="numObs" type="xs:int"/>
					<xs:element name="referencePointName" type="xs:string" minOccurs="0"/>
					<xs:element name="referencePointGeometry" type="gml:GM_Point" minOccurs="0"/>
					<xs:element name="obstacleDefinition" type="pixm:PerformanceObstacleType" minOccurs="0" maxOccurs="unbound"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:element name="PerformanceObstacleSet" type="pixm:PerformanceObstacleSetType" substitutionGroup="pixm:AbstractPIXMFeatureType"/> 	
	<xs:complexType name="PerformanceObstacleSetType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:PerformanceObstacleSetTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:element name="ObstacleAccountabilitySplayTimeSlice" type="ObstacleAccountabilitySplayTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/>
	<xs:complexType name="ObstacleAccountabilitySplayTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="expandedSplayForTurns" type="pixm:CodeYesNoType"/>
					<xs:element name="initialHalfWidth" type="pixm:ValDistanceType"/>
					<xs:element name="maxHalfWidthAfterTurn" type="pixm:ValDistanceType"/>
					<xs:element name="maxTypicalHalfWidth" type="pixm:ValDistanceType"/>
					<xs:element name="oeiSplaySelection" type="pixm:CodeOeiSplaySelectionType"/>
					<xs:element name="oeiSplayStart" type="pixm:CodeOeiSplayStartType"/>
					<xs:element name="splayTurningGrowthRate" type="pixm:ValPercentType"/>
					<xs:element name="splayTypicalGrowthRate" type="pixm:ValPercentType"/>
					<xs:choice>
						<xs:element name="surfaceExtent_3D" type="gml:SurfaceType"/>
						<xs:element name="surfaceExtent_2D" type="gml:SurfaceType"/>
					</xs:choice>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:element name="ObstacleAccountabilitySplay" type="pixm:ObstacleAccountabilitySplayType" substitutionGroup="pixm:AbstractPIXMFeatureType"/> 	
	<xs:complexType name="ObstacleAccountabilitySplayType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:ObstacleAccountabilitySplayTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	
	<xs:element name="PerformanceRunwayTimeSlice" type="PerformanceRunwayTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/>
	<xs:complexType name="PerformanceRunwayTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="asda" type="pixm:ValDistanceType" minOccurs="0"/>
					<xs:element name="asdaSlope" type="pixm:ValSlopeType" minOccurs="0"/>
					<xs:element name="clearwayDistance" type="pixm:ValDistanceType" minOccurs="0"/>
					<xs:element name="elevation" type="pixm:ValDistanceVerticalType" minOccurs="0"/>
					<xs:element name="fixedCBRValue" type="xs:positiveInteger" minOccurs="0"/>
					<xs:element name="lda" type="pixm:ValDistanceType" minOccurs="0"/>
					<xs:element name="ldaSlope" type="pixm:ValSlopeType" minOccurs="0"/>
					<xs:element name="perfRunwaySurface" type="pixm:CodePerfRunwaySurfaceType" minOccurs="0"/>
					<xs:element name="perfStopwaySurface" type="pixm:CodePerfRunwaySurfaceType" minOccurs="0"/>
					<xs:element name="runwayWidth" type="pixm:ValDistanceType" minOccurs="0"/>
					<xs:element name="stopwayDistance" type="pixm:ValDistanceType" minOccurs="0"/>
					<xs:element name="toda" type="pixm:ValDistanceType" minOccurs="0"/>
					<xs:element name="todaSlope" type="pixm:ValSlopeType" minOccurs="0"/>
					<xs:element name="tora" type="pixm:ValDistanceType" minOccurs="0"/>
					<xs:element name="contamDefinition" type="ContaminationDefinitionType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="splayDefinition" type="ObstacleAccountabilitySplayType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="obstacleSet" type="PerformanceObstacleSetType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="aixmRunway" type="aixm:RunwayDirection" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:element name="PerformanceRunway" type="pixm:PerformanceRunwayType" substitutionGroup="pixm:AbstractPIXMFeatureType"/> 	
	<xs:complexType name="PerformanceRunwayType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:PerformanceRunwayTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:element name="PerformanceProfilePointTimeSlice" type="PerformanceProfilePointTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/>
	<xs:complexType name="PerformanceProfilePointTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="airspeed" type="pixm:AirspeedType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="distanceFromReference" type="pixm:ValDistanceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="flapConfiguration" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="flightSegment" type="pixm:NoNumberType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="heightAboveReference" type="pixm:ValDistanceVerticalType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="pointId" type="pixm:NoSequenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="pressureAltitude" type="pixm:ValDistanceVerticalType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="slatConfiguration" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="thrustSetting" type="pixm:ValThrustSettingType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="timeElaposed" type="pixm:ValDurationType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="weight" type="pixm:ValWeightType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="geometry" type="gml:Point" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:element name="PerformanceProfilePoint" type="pixm:PerformanceProfilePointType" substitutionGroup="pixm:AbstractPIXMFeatureType"/> 	
	<xs:complexType name="PerformanceProfilePointType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:PerformanceProfilePointTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:element name="FlightPathTurnDefinition" type="pixm:FlightPathTurnDefinitionType" substitutionGroup="pixm:AbstractPIXMFeatureType"/> 	
	<xs:complexType name="FlightPathTurnDefinitionType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:FlightPathTurnDefinitionTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:element name="FlightPathTurnDefinitionTimeSlice" type="FlightPathTurnDefinitionTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/>
	<xs:complexType name="FlightPathTurnDefinitionTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="fixedBankAngle" type="pixm:ValAngleType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="fixedGradientLossValue" type="pixm:ValSlopeType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="fixedTurnRadius" type="pixm:ValDistanceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="gradientLossApplication" type="pixm:CodeGradientLossApplicationType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="maxBankAngle" type="pixm:ValAngleType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="minTurnStartDistance" type="pixm:ValDistanceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="turnEndDistance" type="pixm:ValDistanceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="turnEndPoint" type="gml:Point" minOccurs="0" maxOccurs="1"/>
					<xs:element name="turnId" type="pixm:NoSequenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="turnStartDistance" type="pixm:ValDistanceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="turnStartGrossHeight" type="pixm:ValDistanceVerticalType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="turnStartNetHeight" type="pixm:ValDistanceVerticalType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="turnStartPoint" type="gml:Point" minOccurs="0" maxOccurs="1"/>
					<xs:element name="turnStartPressureAltitude" type="pixm:ValDistanceVerticalType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="turnStartQFEAltitude" type="pixm:ValDistanceVerticalType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="turnStartType" type="pixm:CodeTurnStartType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!--Element for a collection of turn definitions-->
	<xs:element name="FlightPathTurnDefinitionSet" type="FlightPathTurnDefinitionSetType" substitutionGroup="pixm:AbstractPIXMFeatureType"/>
	<xs:complexType name="FlightPathTurnDefinitionSetType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:FlightPathTurnDefinitionSetTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:element name="FlightPathTurnDefinitionSetTimeSlice" type="FlightPathTurnDefinitionSetTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/>
	<xs:complexType name="FlightPathTurnDefinitionSetTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="turnDefinition" type="pixm:FlightPathTurnDefinitionType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="engineInoperativeSelection" type="pixm:CodeEngineInoperativeSelectionType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="gradientLossApplicationType" type="pixm:CodeGradientLossApplicationType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	
	<!--Element for definition of PerformanceProfileSegment which could be a curve or line segment-->
	<xs:element name="PerformanceProfileSegmentTimeSlice" type="pixm:PerformanceProfileSegmentTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/>
	<xs:complexType name="PerformanceProfileSegmentTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="segmentId" type="pixm:NoSequenceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="sesgmentLength" type="pixm:ValDistanceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="geometry" type="gml:LineString" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:element name="PerformanceProfileSegment" type="pixm:PerformanceProfileSegmentType" substitutionGroup="pixm:AbstractPIXMFeatureType"/>
	<xs:complexType name="PerformanceProfileSegmentType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:PerformanceProfileSegmentTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!--Next element is an object that indicates the obstacle clearance of a flight path over a specific obstacle
	The object references an existing PerformanceObstacleType object-->
	<xs:element name="PerformanceObstacleClearanceTimeSlice" type="pixm:PerformanceObstacleClearanceTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/>
	<xs:complexType name="PerformanceObstacleClearanceTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="obstacle" type="pixm:PerformanceObstacleType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="clearanceMargin" type="pixm:ValDistanceVerticalType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="isConsidered" type="pixm:CodeYesNoType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="isLimiting" type="pixm:CodeYesNoType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:element name="PerformanceObstacleClearance" type="pixm:PerformanceObstacleClearanceType" substitutionGroup="pixm:AbstractPIXMFeatureType"/>
	<xs:complexType name="PerformanceObstacleClearanceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:PerformanceObstacleClearanceTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!--Next element for a Performance profile which is a collection of PerformanceProfilePoints and/or-->
	<!--PerformanceProfileSegments-->
	<xs:element name="PerformanceProfileTimeSlice" type="pixm:PerformanceProfileTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/>
	<xs:element name="PerformanceProfile" type="pixm:PerformanceProfileType"></xs:element>
	<xs:complexType name="PerformanceProfileTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="referencePoint" type="gml:Point" minOccurs="0" maxOccurs="1"/>
					<xs:element name="referencePointName" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="numPathSegments" type="xs:int" minOccurs="0" maxOccurs="1"/>
					<xs:element name="profileName" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="profilePoint" type="pixm:PerformanceProfilePointType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="profileSegment" type="pixm:PerformanceProfileSegmentType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="turnDefinitionSet" type="pixm:FlightPathTurnDefinitionSetType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="obstacleClearance" type="pixm:PerformanceObstacleClearanceType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="PerformanceProfileType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:PerformanceProfileTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!-- Next object is a GeometricProfilePoint with simplified characteristics of distance and height above
	reference, flight segment and a point ID-->
	<xs:element name="GeometricProfilePointTimeSlice" type="pixm:GeometricProfilePointTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/>
	<xs:element name="GeometricProfilePoint" type="pixm:GeometricProfilePointType" substitutionGroup="pixm:AbstractPIXMFeatureType"/>
	<xs:complexType name="GeometricProfilePointTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="distanceFromReference" type="pixm:ValDistanceType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="heightAboveReference" type="pixm:ValDistanceVerticalType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="flightSegment" type="pixm:NoNumberType" minOccurs="0" maxOccurs="1"/>
					<xs:element name="pointId" type="pixm:NoSequenceType" minOccurs="0" maxOccurs="1"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="GeometricProfilePointType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:GeometricProfilePointTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<!-- Next object is a GeometricProfileType for a simplifid 2D non-georefernced profile-->
	<xs:element name="GeometricProfileTimeSlice" type="pixm:GeometricProfileTimeSliceType" substitutionGroup="pixm:AbstractPIXMTimeSliceType"/>
	<xs:element name="GeometricProfile" type="pixm:GeometricProfileType" substitutionGroup="pixm:AbstractPIXMFeatureType"/>
	
	<xs:complexType name="GeometricProfileTimeSliceType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMTimeSliceType">
				<xs:sequence>
					<xs:element name="referencePoint" type="gml:Point" minOccurs="0" maxOccurs="1"/>
					<xs:element name="referencePointName" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="numPathSegments" type="xs:int" minOccurs="0" maxOccurs="1"/>
					<xs:element name="profileName" type="xs:string" minOccurs="0" maxOccurs="1"/>
					<xs:element name="profilePoint" type="pixm:GeometricProfilePointType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="obstacleClearance" type="pixm:PerformanceObstacleClearanceType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="GeometricProfileType">
		<xs:complexContent>
			<xs:extension base="pixm:AbstractPIXMFeatureType">
				<xs:sequence>
					<xs:element name="timeSlice" type="pixm:GeometricProfileTimeSliceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
